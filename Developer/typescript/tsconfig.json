{
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
    },
    "exclude": [
        "node_modules"
    ]
}